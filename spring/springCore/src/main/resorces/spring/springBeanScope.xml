<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

       <bean id="message" class="com.mtsmda.springCore.beanScopes.Message" scope="singleton">
              <property name="message" value="My first message"/>
       </bean>

       <bean id="messagePrototype" class="com.mtsmda.springCore.beanScopes.Message" scope="prototype">
              <property name="message" value="My first message"/>
       </bean>

</beans>